<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1"
    android:baselineAligned="false"
    android:orientation="vertical"
    android:id="@+id/exames_activity"
    android:focusableInTouchMode="true"
    android:descendantFocusability="beforeDescendants">
    <include layout="@layout/toolbar"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.9"
        android:scrollbars="vertical">
        <!--Scrollable content here-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingBottom="10dp"
                android:paddingEnd="@dimen/activity_horizontal_margin"
                android:paddingStart="10dp"
                android:visibility="visible">
                <android.support.design.widget.TextInputLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:theme="@style/TextLabelBlue">
                    <android.support.design.widget.TextInputEditText
                        android:id="@+id/hematocrito"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ems="6"
                        android:hint="@string/Hematocrito"
                        android:inputType="number" />
                </android.support.design.widget.TextInputLayout>

                <android.support.design.widget.TextInputLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:theme="@style/TextLabelBlue">
                    <android.support.design.widget.TextInputEditText
                        android:id="@+id/hemoglobina"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ems="6"
                        android:hint="@string/Hemoglobina"
                        android:inputType="number" />
                </android.support.design.widget.TextInputLayout>

                <android.support.design.widget.TextInputLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:theme="@style/TextLabelBlue">
                    <android.support.design.widget.TextInputEditText
                        android:id="@+id/plaquetas"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ems="5"
                        android:hint="@string/Plaquetas"
                        android:inputType="number" />
                </android.support.design.widget.TextInputLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/gray">

            </LinearLayout>

            <!--LEUCOGRAMA-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:id="@+id/leucograma"
                android:visibility="visible">
                <TextView
                    android:id="@+id/leucogramaMenu"
                    android:textAppearance="@style/StyleParaTitulos"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingEnd="0dp"
                    android:paddingStart="@dimen/activity_horizontal_margin"
                    android:text="@string/Leucograma" />
                <RadioGroup
                    android:id="@+id/leucogramaRadioGroup1"
                    android:paddingTop="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingStart="10dp"
                    android:paddingEnd="0dp">
                    <RadioButton
                        android:id="@+id/leucogramaSemResultados"
                        android:theme="@style/radioButtonStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/NaoRealizado"
                        />
                    <RadioButton
                        android:id="@+id/leucogramaNormal"
                        android:theme="@style/radioButtonStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Normal"
                        />
                </RadioGroup>
                <RadioGroup
                    android:id="@+id/leucogramaRadioGroup2"
                    android:paddingTop="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingStart="10dp"
                    android:paddingEnd="0dp">
                    <RadioButton
                        android:id="@+id/leucogramaEmMelhora"
                        android:theme="@style/radioButtonStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/EmMelhora"
                        />
                    <RadioButton
                        android:id="@+id/leucogramaEmPiora"
                        android:theme="@style/radioButtonStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/EmPiora"
                        />
                    <RadioButton
                        android:id="@+id/leucogramaEstavel"
                        android:theme="@style/radioButtonStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Estavel"
                        />
                </RadioGroup>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/gray">

            </LinearLayout>
            <!--LEUCOGRAMA-->

            <!--PCR-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:id="@+id/pcrLinearLayout"
                android:visibility="visible">
                <TextView
                    android:textAppearance="@style/StyleParaTitulos"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingEnd="0dp"
                    android:paddingStart="@dimen/activity_horizontal_margin"
                    android:text="@string/PCR" />
                <RadioGroup
                    android:id="@+id/pcrRadioGroup1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="5dp"
                    android:paddingEnd="0dp"
                    android:paddingStart="10dp"
                    android:paddingTop="5dp">
                    <RadioButton
                        android:id="@+id/pcrEstavel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Estavel"
                        android:theme="@style/radioButtonStyle" />
                    <RadioButton
                        android:id="@+id/pcrElevando"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Elevando"
                        android:theme="@style/radioButtonStyle" />
                </RadioGroup>
                <RadioGroup
                    android:id="@+id/pcrRadioGroup2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="5dp"
                    android:paddingEnd="0dp"
                    android:paddingStart="10dp"
                    android:paddingTop="5dp">
                    <RadioButton
                        android:id="@+id/pcrEmQueda"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/EmQueda"
                        android:theme="@style/radioButtonStyle" />
                    <RadioButton
                        android:id="@+id/pcrSemResultados"
                        android:theme="@style/radioButtonStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/NaoRealizado"
                        />
                </RadioGroup>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/gray">
            </LinearLayout>
            <!--PCR-->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingBottom="10dp"
                android:paddingEnd="@dimen/activity_horizontal_margin"
                android:paddingStart="@dimen/activity_horizontal_margin"
                android:visibility="visible">
                <android.support.design.widget.TextInputLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:theme="@style/TextLabelBlue">
                    <android.support.design.widget.TextInputEditText
                        android:id="@+id/ureia"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ems="6"
                        android:hint="@string/Ureia"
                        android:inputType="number" />
                </android.support.design.widget.TextInputLayout>
                <android.support.design.widget.TextInputLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:theme="@style/TextLabelBlue">
                    <android.support.design.widget.TextInputEditText
                        android:id="@+id/creatinina"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:ems="6"
                        android:hint="@string/Creatinina"
                        android:inputType="number" />
                </android.support.design.widget.TextInputLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/gray"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="?attr/selectableItemBackground"
                android:onClick="eletrolitosOnCLick"
                android:orientation="vertical"
                android:paddingBottom="20dp"
                android:paddingTop="10dp"
                android:visibility="visible">
                <TextView
                    android:textAppearance="@style/StyleParaTitulos"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingEnd="0dp"
                    android:paddingStart="@dimen/activity_horizontal_margin"
                    android:text="@string/Eletrolitos" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/eletrolitosItens"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="visible">
                <!--Potassio-->
                <LinearLayout
                    android:id="@+id/potassioLinearLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingBottom="10dp"
                    android:visibility="visible">
                    <TextView
                        style="@style/StyleParaTitulos"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginEnd="60dp"
                        android:paddingEnd="0dp"
                        android:paddingStart="@dimen/activity_horizontal_margin"
                        android:text="@string/Potassio" />
                    <RadioGroup
                        android:id="@+id/potassioRadioGroup1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingBottom="5dp"
                        android:paddingEnd="0dp"
                        android:paddingStart="10dp"
                        android:paddingTop="5dp">
                        <RadioButton
                            android:id="@+id/potassioNormal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:text="@string/Normal"
                            android:theme="@style/radioButtonStyle" />
                        <RadioButton
                            android:id="@+id/potassioHipocalemia"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start|center"
                            android:gravity="center"
                            android:text="@string/Hipocalemia"
                            android:theme="@style/radioButtonStyle" />
                    </RadioGroup>
                    <RadioGroup
                        android:id="@+id/potassioRadioGroup2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingBottom="5dp"
                        android:paddingEnd="0dp"
                        android:paddingStart="10dp"
                        android:paddingTop="5dp">
                        <RadioButton
                            android:id="@+id/potassioHipercalemia"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:text="@string/Hipercalemia"
                            android:theme="@style/radioButtonStyle" />
                        <RadioButton
                            android:id="@+id/potassioSemResultados"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/NaoRealizado"
                            android:theme="@style/radioButtonStyle" />
                    </RadioGroup>

                </LinearLayout>
                <!--Potassio-->

                <!--Magnesio-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingBottom="10dp"
                    android:id="@+id/magnesioLinearLayout"
                    android:visibility="visible">
                    <TextView
                        android:id="@+id/menuMagnesio"
                        style="@style/StyleParaTitulos"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:paddingEnd="@dimen/activity_horizontal_margin"
                        android:paddingStart="@dimen/activity_horizontal_margin"
                        android:text="@string/Magnesio" />
                    <RadioGroup
                        android:id="@+id/magnesioRadioGroup1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingBottom="5dp"
                        android:paddingEnd="0dp"
                        android:paddingStart="10dp"
                        android:paddingTop="5dp">
                        <RadioButton
                            android:id="@+id/hipermagnesemia"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:text="@string/Hipermagnesemia"
                            android:theme="@style/radioButtonStyle" />
                        <RadioButton
                            android:id="@+id/hipomagnesemia"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/Hipomagnesemia"
                            android:theme="@style/radioButtonStyle" />
                        </RadioGroup>
                    <RadioGroup
                        android:id="@+id/magnesioRadioGroup2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingBottom="5dp"
                        android:paddingEnd="0dp"
                        android:paddingStart="10dp"
                        android:paddingTop="5dp">
                        <RadioButton
                            android:id="@+id/magnesioNormal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:text="@string/Normal"
                            android:theme="@style/radioButtonStyle" />
                        <RadioButton
                            android:id="@+id/magnesioSemResultados"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/NaoRealizado"
                            android:theme="@style/radioButtonStyle" />
                    </RadioGroup>

                </LinearLayout>
                <!--Magnesio-->

                <!--Fosforo-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingBottom="10dp"
                    android:id="@+id/fosforoLinearLayout"
                    android:visibility="visible">
                    <TextView
                        android:id="@+id/menuFosforo"
                        style="@style/StyleParaTitulos"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:paddingEnd="@dimen/activity_horizontal_margin"
                        android:paddingStart="@dimen/activity_horizontal_margin"
                        android:text="@string/Fosforo" />
                    <RadioGroup
                        android:id="@+id/fosforoRadioGroup1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingBottom="5dp"
                        android:paddingEnd="0dp"
                        android:paddingStart="10dp"
                        android:paddingTop="5dp">
                        <RadioButton
                            android:id="@+id/hiperfosfatemia"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:paddingStart="0dp"
                            android:text="@string/Hiperfosfatemia"
                            android:theme="@style/radioButtonStyle" />
                        <RadioButton
                            android:id="@+id/hipofosfatemia"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start|center"
                            android:gravity="center"
                            android:text="@string/Hipofosfatemia"
                            android:theme="@style/radioButtonStyle" />
                        </RadioGroup>
                    <RadioGroup
                        android:id="@+id/fosforoRadioGroup2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingBottom="5dp"
                        android:paddingEnd="0dp"
                        android:paddingStart="10dp"
                        android:paddingTop="5dp">
                        <RadioButton
                            android:id="@+id/fosforoNormal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:text="@string/Normal"
                            android:theme="@style/radioButtonStyle" />
                        <RadioButton
                            android:id="@+id/fosforoSemResultados"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/NaoRealizado"
                            android:theme="@style/radioButtonStyle" />
                    </RadioGroup>
                </LinearLayout>
                <!--Fosforo-->

                <!--Calcio-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingBottom="10dp"
                    android:id="@+id/calcioLinearLayout"
                    android:visibility="visible">
                    <TextView
                        android:id="@+id/menuCalcio"
                        style="@style/StyleParaTitulos"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:paddingEnd="@dimen/activity_horizontal_margin"
                        android:paddingStart="@dimen/activity_horizontal_margin"
                        android:text="@string/Calcio" />
                    <RadioGroup
                        android:id="@+id/calcioRadioGroup1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingBottom="5dp"
                        android:paddingEnd="0dp"
                        android:paddingStart="10dp"
                        android:paddingTop="5dp">
                        <RadioButton
                            android:id="@+id/hipercalcemia"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:paddingStart="0dp"
                            android:text="@string/Hipercalcemia"
                            android:theme="@style/radioButtonStyle" />
                        <RadioButton
                            android:id="@+id/hipocalcemia"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start|center"
                            android:gravity="center"
                            android:text="@string/Hipocalcemia"
                            android:theme="@style/radioButtonStyle" />
                    </RadioGroup>
                    <RadioGroup
                        android:id="@+id/calcioRadioGroup2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingBottom="5dp"
                        android:paddingEnd="0dp"
                        android:paddingStart="10dp"
                        android:paddingTop="5dp">
                        <RadioButton
                            android:id="@+id/calcioNormal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:text="@string/Normal"
                            android:theme="@style/radioButtonStyle" />
                        <RadioButton
                            android:id="@+id/calcioSemResultados"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/NaoRealizado"
                            android:theme="@style/radioButtonStyle" />
                    </RadioGroup>
                </LinearLayout>
                <!--Calcio-->

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/gray">
            </LinearLayout>


            <include layout="@layout/gasometria_arterial" />

            <include layout="@layout/funcao_hepatica" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:id="@+id/lactatoLinearLayout"
                android:visibility="visible">
                <TextView
                    android:textStyle="bold"
                    android:paddingEnd="@dimen/activity_horizontal_margin"
                    android:paddingStart="@dimen/activity_horizontal_margin"
                    style="@style/TextStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|start"
                    android:text="@string/Lactato" />
                <RadioGroup
                    android:id="@+id/lactato"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|start"
                    android:orientation="horizontal"
                    android:paddingStart="10dp"
                    android:paddingEnd="0dp">
                    <RadioButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="@string/Normal"
                        android:theme="@style/radioButtonStyle" />
                    <RadioButton
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="@string/Elevado"
                        android:theme="@style/radioButtonStyle" />
                    <RadioButton
                        android:layout_gravity="center"
                        android:theme="@style/radioButtonStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/NaoRealizado"
                        />
                </RadioGroup>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/gray" />

            <RelativeLayout
                android:visibility="visible"
                android:orientation="vertical"
                android:paddingTop="10dp"
                android:paddingBottom="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="amilaseOnClick"
                android:background="?attr/selectableItemBackground"
                android:paddingStart="@dimen/activity_horizontal_margin"
                android:paddingEnd="@dimen/activity_horizontal_margin"
                >
                <TextView
                    android:textStyle="bold"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="@string/Amilase"
                    style="@style/TextStyle"
                    />
                <android.support.v7.widget.AppCompatCheckBox
                    android:id="@+id/checkboxAmilase"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:gravity="end"
                    android:theme="@style/CheckBoxStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </RelativeLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:id="@+id/amilaseLinearLayout">
                <RadioGroup
                    android:paddingBottom="10dp"
                    android:paddingStart="10dp"
                    android:paddingEnd="0dp"
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/amilaseItens"
                    android:visibility="gone">
                    <RadioButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Elevada"
                        android:theme="@style/radioButtonStyle" />
                    <RadioButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Normal"
                        android:theme="@style/radioButtonStyle"
                        />
                </RadioGroup>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/gray" />

            <!--Albumina-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:id="@+id/albuminaLinearLayout"
                android:visibility="visible">
                <TextView
                    android:textStyle="bold"
                    android:id="@+id/menuAlbumina"
                    style="@style/StyleParaTitulos"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:paddingEnd="@dimen/activity_horizontal_margin"
                    android:paddingStart="@dimen/activity_horizontal_margin"
                    android:text="@string/Albumina" />
                <RadioGroup
                    android:id="@+id/albuminaRadioGroup1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="5dp"
                    android:paddingEnd="0dp"
                    android:paddingStart="10dp"
                    android:paddingTop="5dp">
                    <RadioButton
                        android:id="@+id/hiperalbuminemia"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="@string/Hiperalbuminemia"
                        android:theme="@style/radioButtonStyle" />
                    <RadioButton
                        android:id="@+id/hipoalbuminemia"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Hipoalbuminemia"
                        android:theme="@style/radioButtonStyle" />
                </RadioGroup>
                <RadioGroup
                    android:id="@+id/albuminaRadioGroup2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="5dp"
                    android:paddingEnd="0dp"
                    android:paddingStart="10dp"
                    android:paddingTop="5dp">
                    <RadioButton
                        android:id="@+id/albuminaNormal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Normal"
                        android:theme="@style/radioButtonStyle" />
                    <RadioButton
                        android:id="@+id/albuminaSemResultados"
                        android:theme="@style/radioButtonStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/NaoRealizado"
                        />
                </RadioGroup>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/gray"/>
            <!--Albumina-->

            <LinearLayout
                android:id="@+id/raioxToraxLayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:paddingBottom="10dp"
                android:paddingTop="10dp">
                <TextView
                    android:textStyle="bold"
                    android:id="@+id/raioxMenu"
                    style="@style/StyleParaTitulos"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingEnd="0dp"
                    android:paddingStart="@dimen/activity_horizontal_margin"
                    android:text="@string/RaioxTorax" />
                <RadioGroup
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="5dp"
                    android:paddingEnd="0dp"
                    android:paddingStart="10dp"
                    android:paddingTop="5dp"
                    android:id="@+id/raioxToraxRadioGroup1">
                    <RadioButton
                        android:id="@+id/raioxNormal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Normal"
                        android:theme="@style/radioButtonStyle" />
                    <RadioButton
                        android:id="@+id/pneumotorax"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Pneumotorax"
                        android:theme="@style/radioButtonStyle" />
                    <RadioButton
                        android:id="@+id/actelectasia"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Actelectasia"
                        android:theme="@style/radioButtonStyle" />
                </RadioGroup>
                <RadioGroup
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingBottom="5dp"
                    android:paddingEnd="@dimen/activity_horizontal_margin"
                    android:paddingStart="10dp"
                    android:paddingTop="5dp"
                    android:id="@+id/raioxToraxRadioGroup2">
                    <RadioButton
                        android:id="@+id/infiltradosNovos"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/InfiltradosNovos"
                        android:theme="@style/radioButtonStyle" />
                    <RadioButton
                        android:layout_gravity="center"
                        android:id="@+id/raioxToraxSemResultados"
                        android:theme="@style/radioButtonStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/NaoRealizado"
                        />
                </RadioGroup>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/raioxToraxItensLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingStart="10dp"
                android:paddingEnd="@dimen/activity_horizontal_margin"
                android:paddingBottom="10dp"
                android:orientation="vertical"
                android:visibility="gone">
                <RelativeLayout
                    android:paddingTop="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">
                    <android.support.v7.widget.AppCompatCheckBox
                        android:theme="@style/CheckBoxStyle"
                        android:id="@+id/crepitacoesApice"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Apice"/>
                    <android.support.v7.widget.AppCompatCheckBox
                        android:theme="@style/CheckBoxStyle"
                        android:layout_toEndOf="@id/crepitacoesApice"
                        android:id="@+id/crepitacoesBaseDireita"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/BaseDireita"/>
                    <android.support.v7.widget.AppCompatCheckBox
                        android:theme="@style/CheckBoxStyle"
                        android:layout_toEndOf="@id/crepitacoesBaseDireita"
                        android:id="@+id/crepitacoesBaseEsquerda"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/BaseEsquerda"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">
                    <android.support.v7.widget.AppCompatCheckBox
                        android:theme="@style/CheckBoxStyle"
                        android:id="@+id/crepitacoesTercoMedioDireito"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/TercoMedioDireito"/>
                    <android.support.v7.widget.AppCompatCheckBox
                        android:theme="@style/CheckBoxStyle"
                        android:layout_toEndOf="@id/crepitacoesTercoMedioDireito"
                        android:id="@+id/crepitacoesTercoMedioEsquerdo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/TercoMedioEsquerdo"/>
                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/gray"/>

        </LinearLayout>

    </ScrollView>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.1">
        <include layout="@layout/bottom_buttons_navigate_fichas"/>
    </RelativeLayout>

</LinearLayout>
